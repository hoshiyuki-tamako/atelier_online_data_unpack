              th #{that.i18nService.i18n.__("風")}
              th #{that.i18nService.i18n.__("土")}
              th #{that.i18nService.i18n.__("光")}
              th #{that.i18nService.i18n.__("闇")}
              th #{that.i18nService.i18n.__("画像")}
          tbody
            - for (const [i, _enemy] of byEnemyState[state].entries())
              - const EXP = that.logicService.state(_enemy.sParam.SPEC.EXP, 80)
              - const HP = that.logicService.state(_enemy.sParam.SPEC.HP, 80)
              - const SATK = that.logicService.state(_enemy.sParam.SPEC.SATK, 80)
              - const SDEF = that.logicService.state(_enemy.sParam.SPEC.SDEF, 80)
              - const MATK = that.logicService.state(_enemy.sParam.SPEC.MATK, 80)
              - const MDEF = that.logicService.state(_enemy.sParam.SPEC.MDEF, 80)
              - const SPD = that.logicService.state(_enemy.sParam.SPEC.SPD, 80)
              tr
                td= i + 1
                td= _enemy.DF
                td= _enemy.strName
                th= EXP
                td= HP
                td #{SATK}
                td #{SDEF}
                td #{MATK}
                td #{MDEF}
                td #{SPD}
                td= HP + SATK + SDEF + MATK + MDEF + SPD
                th #{_enemy.sParam.ELM.FIRE}
                th #{_enemy.sParam.ELM.WATER}
                th #{_enemy.sParam.ELM.WIND}
                th #{_enemy.sParam.ELM.EARTH}
                th #{_enemy.sParam.ELM.LIGHT}
                th #{_enemy.sParam.ELM.DARK}
                td
                  a(href="enemy.html#DF-" + _enemy.DF target="_blank")
                    img(src= that.exportService.rootFolderRelativePath + "img/enemy/Texture2D/" + `enemy_tex_${_enemy.sParam.MDL}.png` alt= _enemy.strName)
        hr

