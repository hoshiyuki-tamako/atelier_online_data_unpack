doctype html
html
  head
    title #{that.i18nService.i18n.__("ランキング")}
    link(rel="stylesheet" href= that.exportService.rootFolderRelativePath + "css/main.css")

    //- scroll top button for mobile
    script(src="https://code.jquery.com/jquery-3.5.1.min.js" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/material-scrolltop/1.0.2/material-scrolltop.min.js" integrity="sha512-U5XMCa20kp0+xaI+DDNcEjTrwecJ5jkWGUB9Ye4cRkGWgfQ03dnXu+eFr7BtjyjPSBCBngip9MlTgHjuSrxXAA==" crossorigin="anonymous")
    link(rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/material-scrolltop/1.0.2/material-scrolltop.min.css" integrity="sha512-V5Tr6yGfJc51zNqUUn+CydMuxKdQBf52F6EX89kityPpmSBduIBl39xsmcoKcmlLds+O8l+qN6FtgSjBSIMspA==" crossorigin="anonymous")
    script(src="https://cdnjs.cloudflare.com/ajax/libs/mobile-detect/1.4.4/mobile-detect.min.js" integrity="sha512-hSVyGuXBHmZy+A+P21DRUAuc7UTfcqc0geieb/H1ZmqiJAoDTfzFYIOBAMTcFUWU/PwLPEoREYG/77ckPB6oSw==" crossorigin="anonymous")
    script(src= that.exportService.rootFolderRelativePath + "js/scrollTopButton.js")

    style.
      table {
        width: 100%;
      }
      th {
        white-space: nowrap;
      }
      td, tbody tr th {
        border-bottom: 1px solid black;
      }
      th, td {
        text-align: center;
      }

      .category-indexes-container {
        display: flex;
        flex-wrap: wrap;
      }
      .category-index-container {
        padding: 12px;
      }

      td img {
        width: 140px;
      }
  body
    div
      h2 #{that.i18nService.i18n.__("TOP")} 100
      p
        small #{that.i18nService.i18n.__("全裝備")} LV 80
      p
        small #{that.i18nService.i18n.__("キャラクター")} LV 80 #{that.i18nService.i18n.__("完食")}
      p
        small #{that.i18nService.i18n.__("敵")} LV 120
    hr

    div
      h3 #{that.i18nService.i18n.__("サブ裝備")}
      div.category-indexes-container
        - for (const state of itemStates)
          div.category-index-container
            h3
              a(href="#item-state-" + state)= that.lookup.state[state] || that.i18nService.i18n.__("総戦闘力")
      div.category-indexes-container
        - for (const element of Object.keys(byItemElement))
          div.category-index-container
            h3
              a(href="#item-element-" + element)= that.lookup.element[element] || that.i18nService.i18n.__("全屬性")
    hr
    div
      h3 #{that.i18nService.i18n.__("キャラクター")}
      div.category-indexes-container
        - for (const state of characterStates)
          div.category-index-container
            h3
              a(href="#chara-" + state)=  that.lookup.state[state] || that.i18nService.i18n.__("総戦闘力")
    hr
    div
      h3 #{that.i18nService.i18n.__("敵")}
      div.category-indexes-container
        - for (const state of enemyStates)
          div.category-index-container
            h3
              a(href="#enemy-state-" + state)=  that.lookup.state[state] || that.i18nService.i18n.__("総戦闘力")
      div.category-indexes-container
        - for (const element of Object.keys(byEnemyElement))
          div.category-index-container
            h3
              a(href="#enemy-element-" + element)= that.lookup.element[element] || that.i18nService.i18n.__("全屬性")
    hr
    div
      - for (const state of itemStates)
        h3(id="item-state-" + state)= that.lookup.state[state] || that.i18nService.i18n.__("総戦闘力")
        table
          thead
            tr
              th #{that.i18nService.i18n.__("ランク")}
              th DF
              th #{that.i18nService.i18n.__("名前")}
              th #{that.i18nService.i18n.__("物理攻撃")}
              th #{that.i18nService.i18n.__("物理防禦")}
              th #{that.i18nService.i18n.__("魔法攻撃")}
              th #{that.i18nService.i18n.__("魔法防禦")}
              th #{that.i18nService.i18n.__("速度")}
              th #{that.i18nService.i18n.__("総戦闘力")}
              th #{that.i18nService.i18n.__("火")}
              th #{that.i18nService.i18n.__("水")}
              th #{that.i18nService.i18n.__("風")}
              th #{that.i18nService.i18n.__("土")}
              th #{that.i18nService.i18n.__("光")}
              th #{that.i18nService.i18n.__("闇")}
              th #{that.i18nService.i18n.__("画像")}
          tbody
            - for (const [i, _item] of byItemState[state].entries())
              - const SATK = that.logicService.state(_item.EQU.SATK, 80)
              - const SDEF = that.logicService.state(_item.EQU.SDEF, 80)
              - const MATK = that.logicService.state(_item.EQU.MATK, 80)
              - const MDEF = that.logicService.state(_item.EQU.MDEF, 80)
              - const SPD = that.logicService.state(_item.EQU.SPD, 80)
              tr
                td= i + 1
                td= _item.DF
                td 
                  span=_item.NAME
                  - if (_item.GROUP_DF)
                    - const _chara = chara.m_vList.find(p => p.GROUP_DF === _item.GROUP_DF)
                    img.icon-small(src= that.exportService.rootFolderRelativePath + "img/chara/Texture2D/icon_chara_all_" + _chara.DF.toString().padStart(4, "0") + "_00.png" alt= _chara.NAME)
                td #{SATK} / #{that.logicService.supportState(SATK)}
                td #{SDEF} / #{that.logicService.supportState(SDEF)}
                td #{MATK} / #{that.logicService.supportState(MATK)}
                td #{MDEF} / #{that.logicService.supportState(MDEF)}
                td #{SPD} / #{that.logicService.supportState(SPD)}
                td= SATK + SDEF + MATK + MDEF + SPD
                th #{_item.ELM.FIRE} / #{that.logicService.supportState(_item.ELM.FIRE)}
                th #{_item.ELM.WATER} / #{that.logicService.supportState(_item.ELM.WATER)}
                th #{_item.ELM.WIND} / #{that.logicService.supportState(_item.ELM.WIND)}
                th #{_item.ELM.EARTH} / #{that.logicService.supportState(_item.ELM.EARTH)}
                th #{_item.ELM.LIGHT} / #{that.logicService.supportState(_item.ELM.LIGHT)}
                th #{_item.ELM.DARK} / #{that.logicService.supportState(_item.ELM.DARK)}
                td
                  a(href="item.html#DF-" + _item.DF target="_blank")
                    img(src= that.exportService.rootFolderRelativePath + "img/icon_s/Texture2D/icon_item_s_" + _item.DF + ".png" alt= _item.NAME)
        hr
